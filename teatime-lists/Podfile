  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  pod 'Moya', '~> 11.0'
  pod 'Moya/RxSwift', '~> 11.0'
  
  # Utilities
  pod 'Kingfisher', '~> 4.0'
  pod 'TinyConstraints'
  pod 'SwiftyJSON'
  

  # Maps
  pod 'GoogleMaps'
  pod 'GooglePlaces'

  # Pods for teatime-lists

  target 'teatime-listsTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
